{
  "root": true,
  "extends": ["airbnb"],
  "parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true
	},
  "rules": {
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/media-has-caption": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
  }
}

